{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MasterInitMsg",
  "type": "object",
  "required": [
    "buttcoin_address",
    "buttcoin_contract_hash",
    "minting_schedule"
  ],
  "properties": {
    "buttcoin_address": {
      "$ref": "#/definitions/HumanAddr"
    },
    "buttcoin_contract_hash": {
      "type": "string"
    },
    "minting_schedule": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ScheduleUnit"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "ScheduleUnit": {
      "type": "object",
      "required": [
        "end_block",
        "mint_per_block"
      ],
      "properties": {
        "end_block": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "mint_per_block": {
          "$ref": "#/definitions/Uint128"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
